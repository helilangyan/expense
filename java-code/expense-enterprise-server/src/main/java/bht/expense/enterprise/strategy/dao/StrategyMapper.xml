<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bht.expense.enterprise.strategy.dao.StrategyMapper">

    <delete id="del" >
        delete from bus_enterprise_strategy where id = #{id}
    </delete>

    <delete id="dels">
        delete from bus_enterprise_strategy where id in
        <foreach collection="array" item="id" index="index" open="(" close=")" separator=",">
            #{id}
        </foreach>
    </delete>

    <select id="findByEnterpriseId" resultType="bht.expense.enterprise.strategy.entity.StrategyEntity">
        select * from bus_enterprise_strategy where enterprise_id = #{enterpriseId}
    </select>

</mapper>